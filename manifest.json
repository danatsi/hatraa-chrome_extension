{
    "manifest_version": 2,

    "name": "Hatraa",
    "description": "This extension will alert you when in danger.",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["socket.io.js", "reciveAlerts.js"]
    },
    "permissions": [
        "activeTab"
    ]
}